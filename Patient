class Patient {
    private String id;
    private String details;

    // Constructor
    public Patient(String id, String details) {
        this.id = id;
        this.details = details;
    }

    // Getters and Setters
    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getDetails() {
        return details;
    }

    public void setDetails(String details) {
        this.details = details;
    }

    @Override
    public String toString() {
        return "Patient{" +
                "id='" + id + '\'' +
                ", details='" + details + '\'' +
                '}';
    }
}
